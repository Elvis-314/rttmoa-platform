{
	"name": "rttmoa-platform",
	"private": true,
	"version": "2.3.30",
	"description": "rttmoa-platform open source management system",
	"author": {
		"name": "rttmoa",
		"email": "rttmoa@gmail.com",
		"url": "https://github.com/rttmoa"
	},
	"license": "MIT",
	"homepage": "https://github.com/rttmoa",
	"repository": {
		"type": "git",
		"url": "git@github.com:rttmoa/rttmoa-platform.git"
	},
	"bugs": {
		"url": "https://github.com/rttmoa/rttmoa-platform/issues"
	},
	"engines": {
		"node": "v18.18.0"
	},
	"scripts": {
		"start": "vite",
		"serve": "serve -s dist -l 9889",
		"preview_Dev": "npm run build:dev && vite preview",
		"preview_Prod": "npm run build:production && vite preview",
		"build": "vite build",
		"build:dev": "tsc && vite build --mode development",
		"build:test": "tsc && vite build --mode test",
		"build:production": "tsc && vite build --mode production",
		"build:production:debug": "tsc && vite build --mode production --debug",
		"tailwindcss": "npx tailwindcss -o build.css --minify",
		"type:check": "tsc --noEmit --skipLibCheck",
		"lint": "npm run lint:eslint  && npm run lint:prettier",
		"lint:fix": "npm run eslint:fix && npm run stylelint && npm run lint:prettier:fix",
		"lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src/",
		"lint:eslint:fix": "eslint --fix --cache --ext .js,.ts,.tsx --format=pretty ./src",
		"lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
		"lint:prettier:check": "prettier --check \"src/**/*\" --end-of-line auto --ignore-unknown",
		"lint:prettier:fix": "prettier --write \"src/**/*\" --end-of-line auto --ignore-unknown",
		"lint:prettier:fix:all": "prettier --write . --end-of-line auto --ignore-unknown",
		"lint:prettier:single": "prettier --write \"src/views/system/userManage/index.tsx\" --end-of-line auto --ignore-unknown",
		"lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
		"lint:lint-staged": "lint-staged",
		"markdownlint": "markdownlint *.md",
		"markdownlint:fix": "markdownlint --fix *.md",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
		"prepare": "husky install",
		"clean:lock": "rimraf package-lock.json yarn.lock pnpm-lock.yaml",
		"clean:dist": "rimraf ./dist",
		"clean:modules": "rimraf node_modules",
		"push:tags": "git push --follow-tags origin pre-gh-pages",
		"release": "standard-version",
		"release:patch": "standard-version --release-as patch",
		"release:minor": "standard-version --release-as minor",
		"release:major": "standard-version --release-as major",
		"rimraf": "rimraf node_modules",
		"commit": "git add -A && czg && git push",
		"releasePatch": "standard-version --release-as patch",
		"push": "git push --follow-tags origin main",
		"predeploy": "npm run build:production",
		"deploy": "gh-pages -d dist -b gh-pages"
	},
	"browserslist": {
		"production": [
			"> 1%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	},
